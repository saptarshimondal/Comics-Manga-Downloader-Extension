    "copy-webpack-plugin": "^11.0.0",
    "html-loader": "^3.1.0",
    "html-webpack-plugin": "^5.5.0",
    "imports-loader": "^4.0.0",
    "web-ext": "^6.8.0",
    "webextension-polyfill": "^0.9.0",
    "webpack": "^5.73.0",
    "webpack-cli": "^4.9.2"
  },
  "dependencies": {
    "file-saver": "^2.0.5",
    "jspdf": "^2.5.1"
  }
}
