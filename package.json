{
  "name": "comics-manga-downloader",
  "displayName": "Comics / Manga Downloader",
  "version": "1.8.0",
  "description": "A cross browser extension to download comics / manga as pdf file from any site.",
  "author": "Saptarshi Mondal <mondalsaptarshi7@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/saptarshimondal/Comics-Manga-Downloader-Extension"
  },
  "scripts": {
    "start:firefox": "webpack --config webpack.config.js",
    "start:chrome": "webpack --config webpack.config.chrome.js",
    "dev:firefox": "webpack -w --config webpack.config.js",
    "dev:chrome": "webpack -w --config webpack.config.chrome.js",
    "ext": "web-ext run --source-dir ./dist/",
    "pack:firefox": "node scripts/pack-firefox.js",
    "pack:chrome": "node scripts/pack-chrome-runner.js",
    "pack:all": "node scripts/pack-all.js",
    "test": "jest"
  },
  "devDependencies": {
    "@babel/core": "^7.28.6",
    "@babel/preset-env": "^7.28.6",
    "babel-jest": "^30.2.0",
    "babel-loader": "^10.0.0",
    "clean-webpack-plugin": "^4.0.0",
    "copy-webpack-plugin": "^13.0.1",
    "html-loader": "^5.1.0",
    "html-webpack-plugin": "^5.6.5",
    "imports-loader": "^5.0.0",
    "jest": "^30.2.0",
    "web-ext": "^9.2.0",
    "webextension-polyfill": "^0.12.0",
    "webpack": "^5.104.1",
    "webpack-cli": "^6.0.1"
  },
  "dependencies": {
    "file-saver": "^2.0.5",
    "jspdf": "^4.1.0",
    "jszip": "^3.10.1",
    "select2": "^4.1.0-rc.0"
  }
}
