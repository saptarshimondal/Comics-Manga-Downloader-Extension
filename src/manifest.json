{
  "manifest_version": 2,
  "name": "Image2PDF",
  "version": "0.1.0",
  "description": "",
  "author": "Saptarshi Mondal",
  "options_ui": {
    "page": "{{./options/index.html}}"
  },
  "icons": {
    "16": "{{./icon/16.png}}",
    "48": "{{./icon/48.png}}",
    "96": "{{./icon/96.png}}",
    "128": "{{./icon/128.png}}"
  },
  "browser_action": {
    "default_popup": "{{./popup/index.html}}",
    "default_icon": {
      "16": "{{./popup/icon/16.png}}",
      "32": "{{./popup/icon/32.png}}",
      "64": "{{./popup/icon/64.png}}"
    }
  },
  "permissions": [
    "activeTab"
  ]
}
